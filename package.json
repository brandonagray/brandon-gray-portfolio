    "test:watch": "jest --watch",
    "prettier": "npx prettier --single-quote es5 --write './src/**/*.jsx' './test/*.test.js' './test/*.test.jsx'",
    "prettier:check": "prettier --single-quote --list-different 'src/**/*.jsx' 'test/*.test.js' 'test/*.test.jsx'"
  },
