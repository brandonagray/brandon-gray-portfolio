  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint .",
    "test": "jest --coverage && cat ./test/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
